        <TextBlock x:Name="addressLabel" HorizontalAlignment="Left" Margin="60,230,0,0" TextWrapping="Wrap" Text="Address: " VerticalAlignment="Top"/>
        <TextBlock x:Name="cityLabel" HorizontalAlignment="Left" Margin="80,276,0,0" TextWrapping="Wrap" Text="City: " VerticalAlignment="Top"/>
