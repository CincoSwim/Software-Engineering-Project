        <TextBox x:Name="AgeInput" HorizontalAlignment="Left" Height="23" Margin="380,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" MaxLength="3"/>
        <TextBox x:Name="ccNumber" HorizontalAlignment="Left" Height="23" Margin="114,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="385" MaxLength="16"/>
